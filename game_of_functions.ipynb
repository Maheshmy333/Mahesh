   "source": [
    "#### Write a Python function to sum all the numbers in a list."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "id": "803fb350",
   "metadata": {},
   "outputs": [],
   "source": [
    "def sum(l1):\n",
    "    sum = 0\n",
    "    for i in l1:\n",
    "        sum = sum + i\n",
    "    print(sum)\n",
    "    \n"
   ]
  },
